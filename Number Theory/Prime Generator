const int N=1000000;
bool prime[N+1];
vector<int>primes;
void sieve(){
    for(int i=2;i<=N;i++)prime[i]=true;
    
    for(int i=2;i*i<=N;i++) {
        if (prime[i]) {
            for (int j=i*i;j<=N;j+=i){
                prime[i]=false;
            }
        }
    }
    for(int i=2;i<=N;i++) if(prime[i]) primes.push_back(i);
}
