#include <bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0), cin.tie(0), cout.tie(0);
typedef long long int ll;
#define pb push_back
#define all(v) v.begin(), v.end()
#define endl '\n'
const long long M = 1000000007;
 
 
void solve() {
    ll n,m; cin>>n>>m;
    vector<vector<pair<ll,ll>>> v(n+1);
    vector<ll>dis(n+1,LLONG_MAX); dis[1]=0;
    vector<ll>parent(n+1);
 
    for(int i=1;i<=m;i++){
        ll x,y,z; cin>>x>>y>>z;
        v[x].push_back({y,z});
    }
 
    set<pair<ll,ll>>pq;
    pq.insert({0,1});
 
    while(!pq.empty()){
        ll node = (*pq.begin()).second;
        ll dist= (*pq.begin()).first;
 
        pq.erase(pq.begin());
 
        for(auto it:v[node]){
            if(dis[it.first]>it.second+dist){
                pq.erase({dis[it.first],it.first});
                dis[it.first]=it.second+dist;
                pq.insert({dis[it.first],it.first});
            }
        }
    }
    for(int i=1;i<n+1;i++)cout<<dis[i]<<" ";
    cout<<endl;
}
 
int main() {
    optimize();
    solve();
    return 0;
}
